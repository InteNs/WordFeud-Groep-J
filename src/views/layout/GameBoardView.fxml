<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="root" alignment="CENTER" stylesheets="@style.css" xmlns="http://javafx.com/javafx/8.0.65"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.GameBoardView">
    <StackPane fx:id="stackPane">
        <GridPane fx:id="gameBoardGrid" maxHeight="600" maxWidth="600" minHeight="600.0" minWidth="600.0"/>
    </StackPane>
    <HBox fx:id="playerRackGrid" alignment="CENTER" minWidth="608" spacing="6.0" minHeight="84">
        <padding>
            <Insets bottom="2" left="2.0" right="2.0" top="2"/>
        </padding>
    </HBox>
</VBox>
